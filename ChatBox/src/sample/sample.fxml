      </Label>
      <ScrollPane layoutX="26.0" layoutY="58.0" prefHeight="361.0" prefWidth="450.0">
        <content>
            <VBox fx:id="vBox">
               <children>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
        </content>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </ScrollPane>
      <TextField fx:id="txtMensaje" layoutX="26.0" layoutY="430.0" onKeyReleased="#enter" prefHeight="25.0" prefWidth="450.0" promptText="Inserta mensaje" />
